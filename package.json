{
  "name": "proofscan",
  "version": "0.10.52",
  "description": "MCP Server scanner - eliminate black boxes by capturing JSON-RPC from connection to tools/list",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "proofscan": "./dist/cli.js",
    "pfscan": "./dist/cli.js",
    "pfs": "./dist/cli.js",
    "psh": "./dist/cli-shell.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/cli.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "lint": "eslint src",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build && npm test"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "scanner",
    "json-rpc",
    "cli",
    "claude",
    "anthropic",
    "ai",
    "tools"
  ],
  "author": "Proof of Protocol Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/proofofprotocol/proofscan.git"
  },
  "bugs": {
    "url": "https://github.com/proofofprotocol/proofscan/issues"
  },
  "homepage": "https://github.com/proofofprotocol/proofscan#readme",
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@types/better-sqlite3": "^7.6.8",
    "@types/node": "^20.10.0",
    "@types/ora": "^3.1.0",
    "@types/uuid": "^9.0.7",
    "@vitest/coverage-v8": "^1.6.1",
    "eslint": "^9.0.0",
    "typescript": "^5.3.0",
    "typescript-eslint": "^8.0.0",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "@hono/node-server": "^1.19.9",
    "better-sqlite3": "^11.0.0",
    "commander": "^12.0.0",
    "hono": "^4.11.4",
    "ora": "^8.2.0",
    "ulid": "^3.0.2",
    "uuid": "^9.0.1",
    "yaml": "^2.8.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
